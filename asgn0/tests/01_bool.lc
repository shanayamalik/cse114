let TRUE  = \x y -> x
let FALSE = \x y -> y
let ITE   = \b x y -> b x y
let NOT   = \b x y -> b y x
let AND   = \b1 b2 -> ITE b1 b2 FALSE
let OR    = \b1 b2 -> ITE b1 TRUE b2

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval not_false :
  NOT FALSE
  -- (a) fill in your reductions here
  =d> if FALSE then FALSE else TRUE -- definition of NOT
  =d> if False then FALSE else TRUE -- definition of FALSE 
  =d> TRUE

eval and_false_true:
  AND FALSE TRUE
  -- (b) fill in your reductions here
  =d> FALSE

eval or_true_false :
  OR TRUE FALSE
  -- (c) fill in your reductions here
  =d> TRUE
